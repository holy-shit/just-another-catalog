       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<definition name="base" template="/WEB-INF/pages/base.jsp">
		<put-attribute name="pageMenu" value="home" type="string"/>
		<put-attribute name="pageHeader" value="/WEB-INF/fragments/static.jsp"/>
		<put-attribute name="pageJs" value="/WEB-INF/fragments/blank.jsp"/>
		<put-attribute name="pageCss" value="/WEB-INF/fragments/blank.jsp"/>
